// ◮◮◮ GENERATED BY AURORA ◮◮◮
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  groups    Group[]
  username  String
  lastLogin DateTime
  email     String   @unique
  firstName String
  lastName  String
  sirName   String
  avatar    String
  birthday  DateTime
  agreement DateTime
  createdAt DateTime
}

model Group {
  id    Int    @id @default(autoincrement())
  name  String
  users User[]
}